# TikTok Shop Agentic RAG System Configuration

# === Core API Keys ===
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
BRIGHT_DATA_API_KEY=your_bright_data_api_key_here

# === LLM Configuration (Anthropic Primary for MCP) ===
LLM_PROVIDER=anthropic
ANTHROPIC_MODEL=claude-3-5-sonnet-20241022
GEMINI_MODEL=gemini-1.5-flash
OPENAI_MODEL=gpt-4-turbo-preview

# === MCP Configuration ===
MCP_PROVIDER=anthropic
MCP_MODEL=claude-3-5-sonnet-20241022
ENABLE_MCP=true

# === Embedding Configuration ===
EMBEDDING_PROVIDER=openai
EMBEDDING_MODEL=text-embedding-3-small
OPENAI_BASE_URL=https://api.openai.com/v1
GEMINI_BASE_URL=https://generativelanguage.googleapis.com/v1beta

# === Database Configuration ===
# PostgreSQL with pgvector for vector storage
DATABASE_URL=postgresql://username:password@localhost:5432/tiktok_agentic_rag
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=tiktok_agentic_rag
POSTGRES_USER=username
POSTGRES_PASSWORD=password

# === Neo4j Configuration (Knowledge Graph) ===
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_neo4j_password

# === Firebase Configuration ===
FIREBASE_PROJECT_ID=your_firebase_project_id
FIREBASE_PRIVATE_KEY_ID=your_private_key_id
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nyour_private_key_here\n-----END PRIVATE KEY-----\n"
FIREBASE_CLIENT_EMAIL=your_service_account_email
FIREBASE_CLIENT_ID=your_client_id
FIREBASE_AUTH_URI=https://accounts.google.com/o/oauth2/auth
FIREBASE_TOKEN_URI=https://oauth2.googleapis.com/token

# === Alternative Vector Database (Optional) ===
# Uncomment if you prefer Pinecone over pgvector
# PINECONE_API_KEY=your_pinecone_api_key
# PINECONE_INDEX_NAME=tiktok-agentic-rag
# PINECONE_ENVIRONMENT=us-west1-gcp

# === Application Configuration ===
APP_ENV=development
LOG_LEVEL=INFO
DEBUG=true
PORT=8000

# === TikTok Shop Specific Configuration ===
TIKTOK_SHOP_API_KEY=your_tiktok_shop_api_key
TIKTOK_BUSINESS_API_KEY=your_tiktok_business_api_key

# === YouTube Channel Configuration ===
YOUTUBE_CHANNEL_ID=UCYourChannelIdHere  # Will be extracted from API
YOUTUBE_CHANNEL_NAME=Bilal Sirbuland
YOUTUBE_CHANNEL_HANDLE=@BilalSirbuland
YOUTUBE_CHANNEL_URL=https://www.youtube.com/@BilalSirbuland
YOUTUBE_VERIFICATION_REQUIRED=true
YOUTUBE_SUBSCRIBER_COUNT=104000
YOUTUBE_VIDEO_COUNT=132
YOUTUBE_DESCRIPTION=TikTok Shop Mastery (UK & USA)

# === Analytics Platform Configuration ===
# Bright Data MCP Settings (Real Implementation)
BRIGHT_DATA_API_KEY=your_brightdata_api_token_here
BRIGHT_DATA_RATE_LIMIT=100/1h
BRIGHT_DATA_WEB_UNLOCKER_ZONE=mcp_unlocker
BRIGHT_DATA_BROWSER_ZONE=mcp_browser
BRIGHT_DATA_TIMEOUT=300

# Analytics Settings
DEFAULT_ANALYSIS_TYPES=best_sellers,gap_analysis,price_analysis
MAX_RESEARCH_RESULTS=50
ENABLE_AUTO_UI_GENERATION=true
ENABLE_CSV_EXPORT=true

# Data Quality Thresholds
DATA_QUALITY_COMPLETENESS_THRESHOLD=0.8
DATA_QUALITY_ACCURACY_THRESHOLD=0.85
DATA_QUALITY_CONSISTENCY_THRESHOLD=0.9

# === Data Ingestion Configuration ===
MAX_CHUNK_SIZE=2000
CHUNK_SIZE=1000
CHUNK_OVERLAP=200
USE_SEMANTIC_CHUNKING=true

# === Agent Configuration ===
MAX_AGENT_ITERATIONS=10
AGENT_TIMEOUT_SECONDS=300
USE_KNOWLEDGE_GRAPH=true
DEFAULT_SEARCH_LIMIT=10

# === Security Configuration ===
JWT_SECRET_KEY=your_jwt_secret_key
CORS_ORIGINS=http://localhost:3000,https://ttssirbulandgpt.com
ALLOWED_HOSTS=localhost,127.0.0.1,yourdomain.com

# === Rate Limiting ===
RATE_LIMIT_PER_MINUTE=60
RATE_LIMIT_PER_HOUR=1000

# === Monitoring & Analytics ===
ENABLE_ANALYTICS=true
ANALYTICS_ENDPOINT=https://your-analytics-endpoint.com
SENTRY_DSN=your_sentry_dsn_for_error_tracking

# === Cache Configuration ===
REDIS_URL=redis://localhost:6379/0
CACHE_TTL_SECONDS=3600

# === File Storage ===
STORAGE_TYPE=local  # local, s3, gcs
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_S3_BUCKET=your-s3-bucket
GCS_BUCKET=your-gcs-bucket

# === Development Tools ===
ENABLE_DEBUG_TOOLBAR=true
ENABLE_PROFILING=false
LOG_SQL_QUERIES=false

# === Feature Flags ===
ENABLE_GRAPH_SEARCH=true
ENABLE_HYBRID_SEARCH=true
ENABLE_STREAMING_RESPONSES=true
ENABLE_TOOL_TRANSPARENCY=true

# === Performance Tuning ===
DB_POOL_SIZE=10
DB_MAX_OVERFLOW=20
EMBEDDING_BATCH_SIZE=50
GRAPH_QUERY_TIMEOUT=30

# === Backup & Recovery ===
BACKUP_ENABLED=true
BACKUP_SCHEDULE="0 2 * * *"  # Daily at 2 AM
BACKUP_RETENTION_DAYS=30

# === YouTube Channel Configuration (for authorization system) ===
YOUTUBE_CHANNEL_NAME=Your Channel Name
YOUTUBE_CHANNEL_ID=your_channel_id
YOUTUBE_CHANNEL_URL=https://youtube.com/@yourchannel
YOUTUBE_CHANNEL_PROFILE_IMAGE=https://your-channel-image.jpg
